import pandas as pd
df=pd.read_csv('OneDrive/area.csv')
df
x=df[['area']]
x
y=df.price
y
from sklearn.linear_model import LinearRegression
model=LinearRegression()
model.fit(x,y)
import warnings
warnings.filterwarnings("ignore")
op=model.predict([[7000]])
op
import pickle
pickle.dump(model,open('area.pkl','wb'))

